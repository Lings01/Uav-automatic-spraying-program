net: "armor/lenet_train_test.prototxt"       #制定网络文件的路径
test_iter: 50           #测试时执行的迭代次数
test_interval: 500      #迭代500次进行测试
base_lr: 0.01         #基础的学习速率
lr_policy: "inv"
gamma: 0.0001
power: 0.75
display: 1000             #迭代100次便显示一次信息
max_iter: 100000         #一共迭代200000次
momentum: 0.9
weight_decay: 0.0005
snapshot: 10000         #迭代20000次生成一次快照
snapshot_prefix: "/home/chad/caffe/armor/state_zhiwu_sec/lenet_zhiwu"      #生成快照的文件名前缀
solver_mode: GPU       #训练模式为GPU模式
